generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model messages {
  id        String   @id
  content   String
  createdAt DateTime
  user_id   String
  updatedAt DateTime
  users     users    @relation(fields: [user_id], references: [id])
}

model users {
  id         String     @id
  first_name String?
  last_name  String?
  username   String     @unique
  password   String
  nickname   String?
  createdAt  DateTime
  updatedAt  DateTime
  messages   messages[]
}

model blacklist {
  token String @id
}
