generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model messages {
  id         String   @id
  content    String
  created_at DateTime
  user_id    String
  updated_at DateTime
  users      users    @relation(fields: [user_id], references: [id])
}

model users {
  id         String     @id
  first_name String?
  last_name  String?
  username   String     @unique
  password   String
  nickname   String?
  created_at DateTime
  updated_at DateTime
  messages   messages[]
}

model blacklist {
  token String @id
}
