generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model messages {
  id        Int      @id @default(autoincrement())
  content   String
  timestamp DateTime
  user_id   Int?
  users     users?   @relation(fields: [user_id], references: [id])
}

model users {
  id         Int        @id @default(autoincrement())
  first_name String?
  last_name  String?
  username   String     @unique
  password   String
  nickname   String?
  messages   messages[]
}
